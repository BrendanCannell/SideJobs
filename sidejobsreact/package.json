  },
  "homepage": "https://github.com/robertmoore40/SideJobs#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.8.11"
    }
}
